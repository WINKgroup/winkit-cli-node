{
    "urlPrefix": "<%= modelNameLower %>s",
    "routes": [
    {
        "method": "GET",
        "urlPath": "/",
        "handler": "list"
    },
    {
        "method": "GET",
        "urlPath": "/:id",
        "handler": "retrieve"
    },
    {
        "method": "POST",
        "urlPath": "/",
        "handler": "post"
    }
]
}